{
  "name": "niftables-frontend-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "PORT=3009 next build",
    "build:prod": "NODE_ENV=production NODE_CONFIG_ENV=prod yarn build",
    "start": "PORT=3009 next start",
    "audit": "audit-ci --config .audit-ci.json",
    "type-check": "tsc",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "lint": "yarn lint:ts && yarn lint:scss",
    "lint:ts": "eslint --ext .tsx --ext .ts ./src/*/** --no-error-on-unmatched-pattern",
    "lint:ts:fix": "eslint --ext .tsx --ext .ts ./src/*/** --no-error-on-unmatched-pattern --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@headlessui/react": "^1.7.17",
    "@tanstack/react-table": "^8.11.3",
    "@types/qs": "^6.9.7",
    "babel-plugin-react-intl": "^8.2.25",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "intl-messageformat": "^10.5.8",
    "lint-staged": "^13.1.0",
    "next": "13.4.20-canary.13",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.4",
    "next-plugin-svgr": "^1.1.3",
    "qs": "^6.11.0",
    "react": "18.2.0",
    "react-code-input": "^3.10.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.0",
    "react-intl": "^6.5.5",
    "react-select": "^5.8.0",
    "sass": "^1.57.1",
    "swiper": "^11.0.5",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.4",
    "@swc/core": "^1.3.101",
    "@types/classnames": "^2.3.1",
    "@types/node": "18.11.18",
    "@types/react": "^18.2.45",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "audit-ci": "^6.6.1",
    "babel-plugin-formatjs": "^10.5.10",
    "eslint": "8.33.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "stylelint": "^14.16.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix",
    "*.scss": "yarn lint:scss:fix"
  }
}
